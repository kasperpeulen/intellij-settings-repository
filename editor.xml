<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="15" />
    <option name="FONT_FAMILY" value="Operator Mono Kasper" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE:PREVIEW" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="IS_FOLDING_OUTLINE_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="LiveEditOptions" nodeUpdateOnChanges="true" restartIfHotSwapFailed="true" />
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bKasper\b.*" />
    <pattern icon="important" pattern="\btodo\b-\bkasper-high\b:.*" />
    <pattern pattern="\btodo\b-\bkasper\b:.*" />
    <filter name="For Kasper High">
      <pattern index="2" />
    </filter>
    <filter name="For Kasper">
      <pattern index="3" />
      <pattern index="2" />
      <pattern index="1" />
    </filter>
  </component>
</application>