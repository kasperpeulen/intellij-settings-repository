<application>
  <component name="EditorSettings">
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="\btodo\-kasper\: \b.*" />
    <pattern icon="important" pattern="\btodo\-kasper\-high\: \b.*" />
    <filter name="for kasper">
      <pattern index="3" />
      <pattern index="2" />
    </filter>
    <filter name="for kasper high">
      <pattern index="3" />
    </filter>
  </component>
</application>