<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="13" />
    <option name="FONT_FAMILY" value="Operator Mono Kasper" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="LiveEditOptions" chromeUpdateApp="true" chromeUpdateOnChanges="true" nodeUpdateOnChanges="true" restartIfHotSwapFailed="true" />
  <component name="TodoConfiguration">
    <pattern pattern="\bkasper\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="question" useCustomColors="true" pattern="\brefactor\b.*">
      <option name="FOREGROUND" value="57a4c0" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern pattern="\btodo\b.*" />
    <filter name="kasper">
      <pattern index="0" />
    </filter>
  </component>
</application>